    {
      "parameters": {
        "jsCode": "// 1. 获取所有输入数据\nconst allArticles = $input.all(); \nconsole.log('待处理文章总数:', allArticles.length);\n\n// 2. 遍历每个文章，仅保留 content 字段\nconst processedArticles = allArticles.map(item => {\n  const originalArticle = item.json; \n  const content = originalArticle.article_content || originalArticle.content || \"无正文\";\n  \n  // 3. 只返回 content 字段\n  return {\n    content: content\n  };\n});\n\n// 4. 返回处理后的文章数组\nreturn processedArticles.map(article => ({ json: article }));"
      },
