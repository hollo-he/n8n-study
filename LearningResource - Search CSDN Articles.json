    {
      "parameters": {
        "jsCode": "// 读取输入的原始文章数据（兼容多条数据）\nconst rawArticles = $input.all().map(item => item.json);\n\n// 清洗单条数据的函数\nfunction cleanArticle(article) {\n  // 去除 HTML 标签（如 <em>）\n  const stripTags = (str) => str ? str.replace(/<\\/?em>/g, '').trim() : '';\n  \n  // 提取并处理标签，增加类型检查\n  const extractTags = (tagStr) => {\n    if (typeof tagStr !== 'string') {\n      console.log(`Invalid tagStr type: ${typeof tagStr}, value: ${tagStr}`);\n      return []; // 非字符串类型返回空数组\n    }\n    return tagStr.split(';').filter(tag => tag.trim() !== '');\n  };\n\n  return {\n    \"url\": article.url || \"\",\n    \"title\": stripTags(article.title),\n    \"blogID\": article.id || \"\",\n    \"authorName\": `${article.author || \"\"}（昵称：${article.nickname || \"\"}）`,\n    \"publishTimestamp\": article.create_time || 0,\n    \"readCount\": String(article.view || 0),\n    \"favoriteCount\": String(article.collections || 0),\n    \"likeCount\": String(article.digg || 0),\n    \"tag\": extractTags(article.search_tag),\n    \"description\": stripTags(article.description || article.digest || \"\")\n  };\n}\n\n// 批量处理所有文章并返回指定格式\nconst cleanedArticles = rawArticles.map(cleanArticle);\n\nreturn [{\n  json: {\n    \"totalItems\": cleanedArticles.length,\n    \"articles\": cleanedArticles\n  }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
