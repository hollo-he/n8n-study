    {
      "parameters": {
        "jsCode": "try {\n  // 检查是否有至少两个输入项（Mode 正确时，items 会包含两个输入）\n  if (items.length < 2) {\n    throw new Error(\"需要两个输入节点，请检查上游连接\");\n  }\n\n  // 第一个输入：基本信息 + URL（假设是上游第一个节点）\n  const infoNode = items[0].json || {}; \n  const basicInfo = infoNode[\"对应信息\"] || {}; // 提取「对应信息」\n  const url = infoNode.url || \"\"; // 提取 URL\n\n  // 第二个输入：文章内容（假设是上游第二个节点）\n  const contentNode = items[1].json || {}; \n  const content = contentNode.content || \"\"; // 提取文章内容\n\n  // 合并输出（只返回一个结果，合并两个输入的数据）\n  return [\n    {\n      json: {\n        \"基本信息\": basicInfo,\n        \"URL\": url,\n        \"文章内容\": content\n      }\n    }\n  ];\n} catch (error) {\n  console.error(\"合并失败：\", error.message);\n  return [{\"json\": {\"error\": error.message}}];\n}"
      },
